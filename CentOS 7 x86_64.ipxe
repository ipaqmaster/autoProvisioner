#!ipxe
dhcp || shell
set baseUrl
set isoDir
set ksDir
set Distro
initrd ${baseUrl}/${isoDir}/${Distro}/images/pxeboot/initrd.img || shell
kernel ${baseUrl}/${isoDir}/${Distro}/images/pxeboot/vmlinuz raid=noautodetect ip=dhcp boot=live:${baseUrl}/${isoDir}/${Distro:uristring}/LiveOS/squashfs.img initrd=initrd.img inst.repo=${baseUrl}/${isoDir}/${Distro:uristring} ks=${baseUrl}/${ksDir}/${Distro:uristring}.cfg inst.sshd || shell
boot || shell
